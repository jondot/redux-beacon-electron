{
  "defaultSeverity": "error",
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": false,
    "no-console": false,
    "prettier": [
      true,
      {
        "singleQuote": true,
        "semi": false
      }
    ]
  }
}